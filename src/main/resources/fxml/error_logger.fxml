<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane orientation="VERTICAL" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thsst.calvis.editor.controller.ErrorLoggerController">
    <items>
        <AnchorPane>
            <children>
                <TableView fx:id="tableViewErrorLogger" stylesheets="@../css/register.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="colErrorType" sortable="false" minWidth="20" text="Error Type" />
                        <TableColumn fx:id="colErrorCause" sortable="false" minWidth="20" text="Error Cause" />
                        <TableColumn fx:id="colErrorMessage" sortable="false" minWidth="50" text="Error Message" />
                    </columns>
                </TableView>
            </children>
        </AnchorPane>
    </items>
</SplitPane>
